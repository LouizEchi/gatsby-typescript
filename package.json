{
  "name": "gatsby-typescript-boilerplate",
  "description": "Gatsby Typescript boilerplate",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "commit": "git-cz"
  },
  "browserslist": [
    "defaults",
    "ie >=9"
  ],
  "dependencies": {
    "gatsby": "^2.0.45",
    "gatsby-image": "^2.0.20",
    "gatsby-plugin-favicon": "^3.1.4",
    "gatsby-plugin-manifest": "^2.0.8",
    "gatsby-plugin-netlify": "^2.0.5",
    "gatsby-plugin-offline": "^2.0.13",
    "gatsby-plugin-react-helmet": "^3.0.1",
    "gatsby-plugin-sass": "^2.0.4",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-plugin-sitemap": "^2.0.2",
    "gatsby-plugin-typescript": "^2.0.1",
    "gatsby-source-filesystem": "^2.0.8",
    "gatsby-transformer-sharp": "^2.3.16",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-helmet": "^5.2.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^8.3.4",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-helmet": "^5.0.7",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "autoprefixer": "^9.3.1",
    "commitlint": "^8.3.5",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.7",
    "eslint": "^6.8.0",
    "gatsby-plugin-tsconfig-paths": "^1.0.2",
    "git-cz": "^4.3.1",
    "husky": "^1.1.3",
    "lint-staged": "^13.2.2",
    "node-sass": "^4.10.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "stylelint": "^9.8.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.4.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.8.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{js,json}": [
      "prettier --write",
      "git add"
    ],
    "*{.scss}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leachjustin18/gatsby-typescript-boilerplate"
  }
}
